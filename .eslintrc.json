{
    "env": {
        "browser": true,
        "es2021": true
    },
    "root": true,
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle": "off",
        "import/prefer-default-export": "off",
        "no-plusplus": "off",
        "semi": ["error", "never"],
        "comma-dangle": ["error", "never"],
        "no-console": "off",
        "func-names": "off",
        "camelcase": "off"
    }
}
